{
    "_version": "1.60.0",
    "sap.app": {
        "id": "com.keke.chatbot",
        "type": "application",
        "title": "A medical chatbot",
        "description": "A medical chatbot",
        "applicationVersion": {
            "version": "1.0.0"
        }
    },
    
    "sap.ui": {
        "technology": "UI5",
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "dependencies": {
            "minUI5Version": "1.120",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {}
            }
        },
        "rootView": {
            "viewName": "com.keke.chatbot.view.App",
            "type": "XML",
            "id": "app"
        },
        "resources": {
            "css": [
              {
                "uri": "css/style.css"
              }
            ]
        }      
    }
